const _trilogyt._created_at <- current_datetime();

import catalog_sales as catalog_sales;

WHERE
    catalog_sales.date.quarter = 2 and (catalog_sales.date.year = 2001 and ((catalog_sales.bill_customer.state in ('CA', 'WA', 'GA') or (catalog_sales.sales_price > 500 or substring(catalog_sales.bill_customer.zip,1,5) in ('85669', '86197', '88274', '83405', '86475', '85392', '85460', '80348', '81792')))))
SELECT
    catalog_sales.bill_customer.zip,
    sum(catalog_sales.sales_price) -> sales,
ORDER BY
    catalog_sales.bill_customer.zip asc nulls first
LIMIT 100
;

PERSIST dscatalog_sales_bill_customer_zip_catalog_sales_item_id_catalog_sales_order_number_379de208_filtered_on_quarter_year_state_sales_price_zip INTO dscatalog_sales_bill_customer_zip_catalog_sales_item_id_catalog_sales_order_number_379de208_filtered_on_quarter_year_state_sales_price_zip FROM WHERE
    catalog_sales.date.quarter = 2 and (catalog_sales.date.year = 2001 and ((catalog_sales.bill_customer.state in ('CA', 'WA', 'GA') or (catalog_sales.sales_price > 500 or substring(catalog_sales.bill_customer.zip,1,5) in ('85669', '86197', '88274', '83405', '86475', '85392', '85460', '80348', '81792')))))
SELECT
    catalog_sales.bill_customer.zip,
    catalog_sales.item.id,
    catalog_sales.order_number,
    catalog_sales.sales_price,
;

PERSIST dscatalog_sales_bill_customer_zip_2da46067 INTO dscatalog_sales_bill_customer_zip_2da46067 FROM SELECT
    catalog_sales.bill_customer.zip,
    sales,
;