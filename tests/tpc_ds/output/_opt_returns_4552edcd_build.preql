PERSIST dsreturns_customer_text_id_returns_store_id_3beaaf57_filtered_on_total_returns_avg_store_returns INTO dsreturns_customer_text_id_returns_store_id_3beaaf57_filtered_on_total_returns_avg_store_returns FROM WHERE
