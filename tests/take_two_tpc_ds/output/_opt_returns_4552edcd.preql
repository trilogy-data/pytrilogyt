const _trilogyt._created_at <- current_datetime();

import store_returns as returns;

WHERE
    returns.store.state = 'TN' and returns.return_date.year = 2000
SELECT
    returns.customer.text_id,
    --sum(returns.return_amount) -> total_returns,
    --returns.store.id,
    --avg(total_returns) by returns.store.id -> avg_store_returns,
HAVING
    total_returns > (1.2 * avg_store_returns)

ORDER BY
    returns.customer.text_id asc
LIMIT 100
;

PERSIST default.dsreturns.customer.text_id_returns.item.id_returns.store_sales.ticket_number_a09b5495_filtered_on_state_year INTO dsreturns.customer.text_id_returns.item.id_returns.store_sales.ticket_number_a09b5495_filtered_on_state_year FROM WHERE
    returns.store.state = 'TN' and returns.return_date.year = 2000
SELECT
    returns.customer.text_id,
    returns.item.id,
    returns.return_amount,
    returns.store.id,
    returns.store_sales.ticket_number,
;

PERSIST default.dsreturns.customer.text_id_returns.store.id_076b41e9 INTO dsreturns.customer.text_id_returns.store.id_076b41e9 FROM SELECT
    total_returns,
    returns.customer.text_id,
    returns.store.id,
;

PERSIST default.dsreturns.store.id_54404a33 INTO dsreturns.store.id_54404a33 FROM SELECT
    avg_store_returns,
    returns.store.id,
;

PERSIST default.dsreturns.customer.text_id_returns.store.id_3beaaf57_filtered_on_total_returns_avg_store_returns INTO dsreturns.customer.text_id_returns.store.id_3beaaf57_filtered_on_total_returns_avg_store_returns FROM WHERE
    total_returns > (1.2 * avg_store_returns)
SELECT
