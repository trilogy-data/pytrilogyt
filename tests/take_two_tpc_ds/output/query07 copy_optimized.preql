import _trilogyt_opt_dbed68c9eadcf927d1ef0070e8c41dcfd70496020c58c2c1e81a7139d62bd01e;

WHERE
    store_sales.customer_demographic.gender = 'M' and (store_sales.customer_demographic.marital_status = 'S' and (store_sales.customer_demographic.education_status = 'College' and (((store_sales.promotion.channel_email = 'N' or store_sales.promotion.channel_event = 'N')) and store_sales.date.year = 2000)))
SELECT
    store_sales.item.name,
    avg(store_sales.quantity) -> avg_quantity,
    avg(store_sales.list_price) -> avg_list_price,
    avg(store_sales.coupon_amt) -> avg_coupon_amt,
    avg(store_sales.sales_price) -> avg_sales_price,
ORDER BY
    store_sales.item.name asc
LIMIT 100
;